<suite name="Test Suite">
    <tests name="UI and Api Test">
        <test name="Chrome UI Test">
            <parameter name="browser" value="chrome"></parameter>
                <classes>
                    <class name="test.InterfaceTest"/>
                </classes>
        </test>
        <test name="Firefox UI Test">
            <parameter name="browser" value="firefox"></parameter>
            <classes>
                <class name="test.InterfaceTest"/>
            </classes>
        </test>
        <test name="Api Test">
            <classes>
                <class name="test.ApiTest"/>
            </classes>
        </test>
    </tests>
</suite>